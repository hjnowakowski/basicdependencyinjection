<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="mojLogger" class="info.henryk.springframework.implementations.LoggerImpl">
        <property name="name" value="MojLogger"/>
        <property name="version" value="3"/>
    </bean>
    <bean id="repoUzytkownikow" class="info.henryk.springframework.implementations.UsersRepositoryImpl">
        <property name="logger" ref="mojLogger"></property>

    </bean>

</beans>