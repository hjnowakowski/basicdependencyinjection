<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">





    <bean id="user" class="info.henryk.springframework.User">
        <!--<property name="name" value="Janek"/>-->
        <constructor-arg value="Janek"/>
        <property name="adress">
            <bean class="info.henryk.springframework.Address">
                <constructor-arg value="Warszawa"/>
                <constructor-arg value="02-234"/>
                <constructor-arg value="Koszykowa"/>
                <constructor-arg value="23"/>
            </bean>
        </property>
    </bean>



    <bean id="user2" class="info.henryk.springframework.User">
        <constructor-arg><null></null></constructor-arg>

    </bean>
</beans>